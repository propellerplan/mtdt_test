    </formulas>
    <formulas>
        <name>formulaTransfer</name>
        <dataType>String</dataType>
        <expression>&quot; :currency_exchange: &quot; +  &apos;_&apos; + {!Get_Person.Name} + &apos;_&apos; + &quot; &quot; +  &apos;*&apos; + TEXT({!Get_Outcome_Bank.CurrencyIsoCode}) + &quot; &quot; +  TEXT({!$Record.Outcome__c}) + &apos;*&apos; + &quot; (&quot; + {!$Record.Outcome_Bank_Account__r.Name} + &quot;) &quot; + &quot; &quot; + &quot;:arrow_right:&quot; + &quot; &quot; + &apos;*&apos; + TEXT({!Get_Income_Bank.CurrencyIsoCode}) + &quot; &quot; +  TEXT({!$Record.Income__c}) + &apos;*&apos; + &quot; (&quot; + {!$Record.Income_Bank_Account__r.Name} + &quot;) &quot; + BR() + 
