    <formulas>
        <name>formulaIncome</name>
        <dataType>String</dataType>
        <expression>&quot; :chart_with_upwards_trend: &quot; +  &apos;_&apos; + {!Get_Person.Name} + &apos;_&apos; + &quot; &quot; +  &apos;*&apos; + TEXT({!$Record.CurrencyIsoCode}) + &quot; &quot; +  TEXT({!$Record.Income__c}) + &apos;*&apos; + &quot; (&quot; + {!$Record.Income_Bank_Account__r.Name} + &quot;) &quot; + BR() + 
TEXT({!$Record.Income_Category__c})+ &quot;: &quot; + &quot;&lt;https://propellerplan.lightning.force.com/&quot; + {!$Record.Id} + &apos;|*&apos; + {!$Record.Description__c} + &apos;*&gt;&apos;</expression>
    </formulas>
    <formulas>
