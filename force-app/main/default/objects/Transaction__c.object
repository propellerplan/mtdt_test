<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">

<validationRules>
    <fullName>Income_Bank_is_Required</fullName>
    <active>true</active>
    <errorConditionFormula>AND( 
				NOT(ISNULL(Income__c)),
				ISBLANK( Income_Bank_Account__c )
)</errorConditionFormula>
    <errorDisplayField>Income_Bank_Account__c</errorDisplayField>
    <errorMessage>Income Bank is required.</errorMessage>
</validationRules>

<validationRules>
    <fullName>Outcome_Bank_is_Required</fullName>
    <active>true</active>
    <errorConditionFormula>AND( 
				NOT(ISNULL(Outcome__c)),
				ISBLANK( Outcome_Bank_Account__c )
)</errorConditionFormula>
    <errorDisplayField>Outcome_Bank_Account__c</errorDisplayField>
    <errorMessage>Outcome Bank is required.</errorMessage>
</validationRules>
</CustomObject>